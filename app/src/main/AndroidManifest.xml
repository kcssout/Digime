<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.digime">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="me.digi.examples.barebonesapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name="me.digi.examples.barebonesapp.postbox.PostboxActivity"
                android:label="Data exporter">
        </activity>

        <activity
                android:name="me.digi.examples.barebonesapp.consentaccess.ConsentAccessActivity"
                android:label="Shady car insurance">
        </activity>

    </application>



</manifest>